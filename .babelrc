{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-runtime",
    [
      "react-intl",
      {
        "messagesDir": "./src/i18n/translations",
        "enforceDescriptions": false
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}